$TTL 86400
@   IN  SOA ns1.{{ dns_domain }}. admin.{{ dns_domain }}. (
        2025051301 ; Serial
        3600       ; Refresh
        1800       ; Retry
        604800     ; Expire
        86400 )    ; Minimum TTL

@       IN  NS      ns1.{{ dns_domain }}.
ns1     IN  A       192.168.1.10
server1 IN  A       192.168.1.100
